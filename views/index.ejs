    <script>
        function setupPopup(popupId, openBtnClass, closeBtnClass) {
            const popup = document.getElementById(popupId);
            const closeBtn = popup.querySelector(closeBtnClass);
            const openBtns = document.querySelectorAll(openBtnClass);
    
            openBtns.forEach(btn => {
                btn.onclick = function (event) {
                    event.preventDefault();
                    popup.style.display = 'block'; // Show the popup
                }
            });
    
            if (closeBtn) {
                closeBtn.onclick = function () {
                    popup.style.display = 'none'; // Hide the popup
                }
            }
    
            window.onclick = function (event) {
                if (popup && event.target === popup) {
                    popup.style.display = 'none'; // Hide the popup when clicking outside
                }
            }
        }
    
